{
  "name": "cognigy-qelp-helper",
  "version": "0.0.1",
  "description": "Qelp Helper Plugin",
  "scripts": {
     "build": "parcel build src/index.jsx --dist-dir dist"
  },
  "alias": {
    "react": "./alias/react",
    "react/jsx-runtime": "./alias/react-jsx-runtime"
  },
  "targets": {
    "default": {
      "context": "browser",
      "outputFormat": "global",
      "isLibrary": true,
      "distDir": "dist",
      "sourceMap": false
    }
  },  
  "author": "sergio.marreiro@foundever.com",
  "license": "ISC",
  "dependencies": {
    "parcel": "^2.14.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-react": "^7.26.3",
    "babel-plugin-module-resolver": "^5.0.2"
  }
}
